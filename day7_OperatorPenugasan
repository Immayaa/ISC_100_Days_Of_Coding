package immayaa;

public class day7_OperatorPenugasan {

    public static void main(String[] args) {
        //operator penugasan

        int a = 50;
        a += 25;
        System.out.println(a);

        int a1 = 50;
        a1 -= 20;
        System.out.println(a1);

        int a2 = 50;
        a2 /= 15;
        System.out.println(a2);

        int a3 = 50;
        a3 *= 10;
        System.out.println(a3);

        int a4 = 50;
        a4 %= 5;
        System.out.println(a4);

    }

}
